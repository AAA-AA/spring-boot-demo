<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
		PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context defaultModelType="flat" id="write" targetRuntime="MyBatis3">

		<plugin type="com.github.mybatis.generator.plugins.LombokPlugin"/>
		<plugin type="com.github.mybatis.generator.plugins.RenamePlugin" />
		<plugin type="com.github.mybatis.generator.plugins.SeparateUserCodePlugin" />

		<commentGenerator type="com.github.mybatis.generator.comment.CommentGenerator">
			<property name="suppressAllComments" value="false" />
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<jdbcConnection connectionURL="jdbc:mysql://127.0.0.1:3306/demo?useUnicode=true&amp;characterEncoding=utf-8"
			driverClass="com.mysql.jdbc.Driver" password="1101"
			userId="root" />


		<!--实体 -->
		<javaModelGenerator targetPackage="ren.com.cn.domain.entity"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!--mapper映射文件-->
		<sqlMapGenerator targetPackage="mybatis.mappers.mbg"
			targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
			<property name="renamePlugin.prefix" value="MBG" />
			<property name="separateUserCodePlugin.targetPackage" value="mybatis.mappers" />
		</sqlMapGenerator>

		<!--接口mapper -->
		<javaClientGenerator targetPackage="ren.com.cn.dao.mapper.mbg"
			targetProject="src/main/java" type="XMLMAPPER">
			<property name="renamePlugin.prefix" value="MBG" />
			<property name="enableSubPackages" value="true"/>
			<property name="exampleMethodVisibility" value="public"/>
			<property name="methodNameCalculator" value="default"/>
			<!--子接口-->
			<property name="separateUserCodePlugin.targetPackage"
					  value="ren.com.cn.dao.mapper"/>
			<property name="separateUserCodePlugin.search" value="\AMBG"/>
			<property name="separateUserCodePlugin.replace" value=""/>
		</javaClientGenerator>
